{
    "manifest_version" : 2,

    "name" : "Leap for Jira",
    "description" : "A chrome extension for controlling jira with the leap motion controller",
    "version" : "0.1",

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "run_at" : "document_end",
            "js": ["lib/jquery.js", "lib/jqueryColor.js", "lib/leap.js", "app.js"]
        }
    ],

    "browser_action" : {
        "default_icon" : "img/coins.png"
    }
}